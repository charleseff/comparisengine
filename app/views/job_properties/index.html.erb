<h1>Listing job_properties</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Weight</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @job_properties.each do |job_property| %>
  <tr>
    <td><%= job_property.name %></td>
    <td><%= job_property.weight %></td>
    <td><%= link_to 'Show', job_property %></td>
    <td><%= link_to 'Edit', edit_job_property_path(job_property) %></td>
    <td><%= link_to 'Destroy', job_property, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

